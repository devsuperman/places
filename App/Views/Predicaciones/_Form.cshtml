@model Predicacion

<div class="mb-2">
    <label asp-for="Fecha"></label>
    <input class="form-control" asp-for="Fecha" required autofocus  >
    <span asp-validation-for='Fecha' class='text-danger'></span>
</div>

<div class="mb-2">
    <label asp-for="TerritorioId"></label>
    <select class="form-select" asp-for="TerritorioId" asp-items='(SelectList)ViewData["selectTerrotorios"]'>
        <option value=''>Selecione</option>
    </select>
    <span asp-validation-for='TerritorioId' class='text-danger'></span>
</div>

<div class="mb-2">
    <label asp-for="Manzanas"></label>
    <input class="form-control" asp-for="Manzanas">
    <span asp-validation-for='Manzanas' class='text-danger'></span>
</div>